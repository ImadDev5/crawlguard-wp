{
  "name": "paypercrawl-backend",
  "version": "1.0.0",
  "description": "PayPerCrawl Production API Backend - Enterprise AI Bot Detection & Monetization Platform",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:seed": "tsx src/database/seed.ts",
    "docs": "swagger-jsdoc -d swaggerDef.js -o ./docs/swagger.json",
    "postman": "tsx src/utils/generatePostman.ts"
  },
  "keywords": [
    "api",
    "bot-detection",
    "ai-monetization",
    "enterprise",
    "paypercrawl"
  ],
  "author": "PayPerCrawl.tech",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.10.0",
    "@supabase/supabase-js": "^2.39.0",
    "argon2": "^0.31.2",
    "axios": "^1.6.7",
    "bull": "^4.12.0",
    "cloudflare": "^3.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "joi": "^17.12.0",
    "jsonwebtoken": "^9.0.2",
    "maxmind": "^4.3.18",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^5.0.4",
    "nodemailer": "^6.9.8",
    "pino": "^8.18.0",
    "pino-pretty": "^10.3.1",
    "stripe": "^14.14.0",
    "ua-parser-js": "^1.0.37",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.10",
    "@types/nodemailer": "^6.4.14",
    "@types/ua-parser-js": "^0.7.39",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.4",
    "prisma": "^5.10.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
